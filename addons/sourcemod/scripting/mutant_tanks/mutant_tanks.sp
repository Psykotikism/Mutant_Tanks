	return adAddress;
}

int iGetGameDataOffset(const char[] name)
{
	int iOffset = g_esGeneral.g_gdMutantTanks.GetOffset(name);
	if (iOffset == -1)
	{
		LogError("%s Failed to load offset: %s", MT_TAG, name);
	}

	return iOffset;
}

