				g_esPlayer[survivor].g_iMidairDashesLimit += iClamp(g_esPlayer[survivor].g_iMidairDashesLimit);
				g_esPlayer[survivor].g_iStickyGrenades = g_esCache[tank].g_iStickyGrenadesReward[priority];
