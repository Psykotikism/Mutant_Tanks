			for (int iPos = 0; iPos < (sizeof esPlayer::g_iRock); iPos++)
			{
				if (iPos < (sizeof esPlayer::g_iOzTank))
				{
					if (bRainbow2[0] && bIsValidEntRef(g_esPlayer[player].g_iOzTank[iPos]))
					{
