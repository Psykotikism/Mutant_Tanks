			case 4: vCheatCommand(iPlayer, sCommand, "spitter auto");
			case 5: vCheatCommand(iPlayer, sCommand, "jockey auto");
			case 6: vCheatCommand(iPlayer, sCommand, "charger auto");
			default: vCheatCommand(iPlayer, sCommand, "tank auto");
		}
	}
}

