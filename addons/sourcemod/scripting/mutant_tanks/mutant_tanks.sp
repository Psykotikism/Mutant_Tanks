		{
			FormatEx(sName, sizeof sName, "MTPatch_TankAttackDifficulty%i", (iPos + 1));
			iIndex[iPos] = iGetPatchIndex(sName);
		}

		if (iIndex[iPos] != -1)
		{
			vRemovePatch(iIndex[iPos]);
		}
	}

	return MRES_Ignored;
}

