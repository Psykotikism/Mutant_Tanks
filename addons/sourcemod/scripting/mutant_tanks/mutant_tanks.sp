void vRespawnInfectedPlayers(bool extra = true)
{
	for (int iSpecial = 1; iSpecial <= MaxClients; iSpecial++)
	{
		if (bIsHumanInfected(iSpecial, MT_CHECK_INGAME) && !bIsValidClient(iSpecial, MT_CHECK_ALIVE))
